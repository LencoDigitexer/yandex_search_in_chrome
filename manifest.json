{
    "name": "Поиск в Яндексе из контекстного меню",
    "version": "0.8",
    "manifest_version": 2,
    "description": "This extensions helps you to search information with yandex from context menu",
    "permissions": ["contextMenus"],
    "background": {
        "scripts": ["code.js"]
    },
    "icons": {
        "16": "icons/16.png",
        "32": "icons/32.png",
        "48": "icons/48.png",
        "64": "icons/64.png",
        "128": "icons/128.png"
    }
}
